    "license": "ISC",
    "private": true,
    "scripts": {
        "dev": "cross-env NODE_ENV=development && tsnd --transpile-only --ignore-watch node_modules --respawn src/server.ts",
        "start": "cross-env NODE_ENV=production && node build/server.js",
        "seed:dev": "cross-env NODE_ENV=development && tsnd src/database/seed.ts",
        "seed:prod": "cross-env NODE_ENV=production && node build/database/seed.js",
        "build": "tsc -p tsconfig.json",
        "test": "cross-env NODE_ENV=test && mocha -r ts-node/register src/tests/*.test.ts",
        "graphdocs": "graphdoc -e http://localhost:4000/graphql -f -o ./public/docs",
        "docs": "jsdoc -c jsdoc.json",
        "cross-env": "cross-env"
    },
    "dependencies": {
        "apollo-server-express": "^2.24.0",
        "aws-sdk": "^2.899.0",
        "bcrypt": "^5.0.0",
        "bunyan": "^1.8.15",
        "class-validator": "^0.13.0",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dotenv": "^10.0.0",
        "express-async-errors": "^3.1.1",
        "graphql": "^15.4.0",
        "handlebars": "^4.7.7",
        "js-sha256": "^0.9.0",
        "jsonwebtoken": "^8.5.1",
        "md5": "^2.3.0",
        "moment": "^2.29.1",
        "moment-timezone": "^0.5.32",
        "mongodb": "^3.6.6",
        "nodemailer": "^6.6.0",
        "reflect-metadata": "^0.1.13",
        "sharp": "^0.28.1",
        "slugify": "^1.5.1",
        "subscriptions-transport-ws": "^0.9.18",
        "type-graphql": "^1.1.1",
        "typeorm": "^0.2.32",
        "typescript": "^4.1.3",
        "yup": "^0.32.8"
    },
    "devDependencies": {
        "@2fd/graphdoc": "^2.4.0",
        "@types/aws-sdk": "^2.7.0",
        "@types/bcrypt": "^3.0.0",
        "@types/bunyan": "^1.8.6",
        "@types/chai": "^4.2.15",
        "@types/cors": "^2.8.9",
        "@types/dotenv": "^8.2.0",
        "@types/jest": "^27.0.2",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/md5": "^2.2.1",
        "@types/mocha": "^8.2.1",
        "@types/moment": "^2.13.0",
        "@types/moment-timezone": "^0.5.30",
        "@types/mongodb": "^3.6.8",
        "@types/node": "^15.0.1",
        "@types/nodemailer": "^6.4.0",
        "@types/reflect-metadata": "^0.1.0",
        "@types/sharp": "^0.28.0",
        "@types/supertest": "^2.0.10",
        "@types/yup": "^0.29.11",
        "better-docs": "^2.3.2",
        "chai": "^4.3.4",
        "jsdoc": "^3.6.6",
        "mocha": "^8.3.2",
        "supertest": "^6.1.3",
        "ts-node": "^9.1.1",
        "ts-node-dev": "^1.1.1"
    }
